# PostgreSQL connection string. Production: use the URL from Vercel (Neon/Postgres).
# Local dev: use a Neon dev database or local Postgres, e.g. postgresql://user:pass@localhost:5432/dbname
DATABASE_URL=""

# NextAuth (required for JWT signing; use: openssl rand -base64 32)
# Use either NEXTAUTH_SECRET or AUTH_SECRET (no extra text like "BETTER_AUTH_SECRET=")
NEXTAUTH_SECRET=""
NEXTAUTH_URL="http://localhost:3000"

# Google OAuth (create at https://console.cloud.google.com/apis/credentials)
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""
